{
    "name": "florentpoujol/php-standard-components",
    "description": "A series of libraries/components that form the basics of many website and most frameworks.",
    "authors": [
        {
            "name": "Florent Poujol",
            "email": "florent.poujol@gmail.com"
        }
    ],

    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },

    "autoload": {
        "psr-4": {
            "StdCmp\\": "src",
            "Tests\\": "tests"
        }
    },

    "require": {
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/cache": "^1.0",
        "psr/container": "^1.0"
    },

    "provide": {
        "psr/log-implementation": "1.0",
        "psr/simple-cache-implementation": "1.0",
        "psr/cache-implementation": "1.0",
        "psr/container-implementation": "1.0"
    }
}
